{
  "name": "face-blinder",
  "version": "0.0.2",
  "description": "Assitant Bot for Whom is Suffering form Face Blindness",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "lint": "tslint --project tsconfig.json && npm run clean && tsc --noEmit",
    "test": "tape -r ts-node/register \"src/**/*.spec.ts\" \"test/**/*.spec.ts\"",
    "test:npm": "cross-env-shell TMPDIR=/tmp/npm-package-test.$$ 'npm pack && mkdir $TMPDIR && mv *-*.*.*.tgz $TMPDIR && cp tests/fixtures/smoke-testing.js $TMPDIR && cd $TMPDIR && npm init -y && npm i *-*.*.*.tgz && node smoke-testing.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zixia/face-blinder.git"
  },
  "keywords": [],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zixia/face-blinder/issues"
  },
  "homepage": "https://github.com/zixia/face-blinder#readme",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "facenet": "^0.3.8",
    "flash-store": "0.0.9",
    "numjs": "^0.14.2"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/blue-tape": "^0.1.31",
    "@types/node": "^8.0.28",
    "@types/numjs": "^0.14.2",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^2.3.5",
    "@types/sinon-test": "^1.0.2",
    "blue-tape": "^1.0.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.7.2",
    "shx": "^0.2.2",
    "sinon": "^3.3.0",
    "sinon-test": "^2.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "files": [
    "bin/",
    "dist/",
    "src/",
    "CHANGELOG",
    "LICENSE",
    "package.json",
    "README.md"
  ]
}
